    "host": "192.168.3.180:8081",
    "basePath": "/api/v1",
    "paths": {
        "/pics": {
            "post": {
                "description": "上传美女图集接口",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "上传"
                ],
                "summary": "上传图集接口",
                "parameters": [
                    {
                        "description": "参数列表",
                        "name": "object",
                        "in": "body",
                        "schema": {
                            "$ref": "#/definitions/models.Pics"
                        }
                    }
                ],
                "responses": {
                    "1001": {
                        "description": "参数错误",
                        "schema": {
                            "$ref": "#/definitions/controllers._Responsefailure"
                        }
                    },
                    "1002": {
                        "description": "服务繁忙",
                        "schema": {
                            "$ref": "#/definitions/controllers._Responsefailure"
                        }
                    },
                    "200": {
                        "description": "请求成功",
                        "schema": {
                            "$ref": "#/definitions/controllers._ResponseSuccess"
