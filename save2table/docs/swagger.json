                    }
                }
            }
        }
    },
    "definitions": {
        "controllers._ResponseSuccess": {
            "type": "object",
            "properties": {
                "code": {
                    "description": "状态码",
                    "type": "integer"
                },
                "data": {
                    "description": "数据",
                    "type": "object",
                    "$ref": "#/definitions/models.Pics"
                },
                "msg": {
                    "description": "提示信息",
                    "type": "string"
                }
            }
        },
        "controllers._Responsefailure": {
            "type": "object",
            "properties": {
                "code": {
                    "description": "状态码",
                    "type": "integer"
                },
                "msg": {
                    "description": "提示信息",
                    "type": "string"
                }
            }
        },
        "models.Pics": {
            "type": "object",
            "properties": {
                "category": {
                    "description": "选填：分类，没有就传空",
                    "type": "string",
                    "example": "清纯"
                },
                "description": {
                    "description": "选填：图集描述，没有就传空",
                    "type": "string",
                    "example": "这里是描述"
                },
                "pics": {
                    "description": "必填：图片链接，存在1个以上请用'｜'拼接",
                    "type": "string",
                    "example": "xxx1.jpg|xxx2.jpg|xxx3.jpg"
                },
                "tags": {
                    "description": "选填：标签，没有就传空，存在1个以上请用'｜'拼接，",
                    "type": "string",
                    "example": "标签1|标签2|标签3"
                },
                "title": {
                    "description": "必填：图集标题",
                    "type": "string",
                    "example": "清纯美少女"
                },
                "webside": {
                    "description": "必填：网站拼音或者缩写",
                    "type": "string",
                    "example": "baidu"
                }
            }
        }
    }
